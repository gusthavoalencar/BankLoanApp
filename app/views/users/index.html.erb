<h1>Users#index</h1>
<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Phone Number</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.firstName %></td>
        <td><%= user.lastName %></td>
        <td><%= user.email %></td>
        <td><%= user.phoneNumber %></td>
        <td>
          <%= link_to 'Show', user_path(user) %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>
